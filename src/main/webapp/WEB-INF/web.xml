<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>projeto-web-bootstrap-tiles</display-name>
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>sairServlet</display-name>
    <servlet-name>sairServlet</servlet-name>
    <servlet-class>br.com.projeto.empresa.controller.sairServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sairServlet</servlet-name>
    <url-pattern>/sairServlet</url-pattern>
  </servlet-mapping>
 
</web-app>